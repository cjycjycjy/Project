package ddoraemi.creategroup.view;

public class CalData {
	int day;
	int dayofweek;
	int month;
	int year;
	boolean isPossibleCreate;
	boolean isPerformDay;

	public CalData(int d, int h, int y, int m) {
		day = d;
		dayofweek = h;
		year = y;
		month = m;
		isPossibleCreate=false;
	}
	public void setisPossibleCreateTrue(boolean create)
	{
		isPossibleCreate=create;
	}
	public boolean getisPossibleCreate()
	{
		return isPossibleCreate;
	}

	public int getMonth() {
		return month;
	}

	public int getYear() {
		return year;
	}

	public int getDay() {
		return day;
	}

	public int getDayofweek() {
		return dayofweek;
	}

	public boolean isPerformDay() {
		return isPerformDay;
	}
	public void setPerformDay(boolean isPerformDay) {
		this.isPerformDay = isPerformDay;
	}
}
