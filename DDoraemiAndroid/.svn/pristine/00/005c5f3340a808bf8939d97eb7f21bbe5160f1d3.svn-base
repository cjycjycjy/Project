<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/tv_challenge_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="21dp"
                android:textColor="#795548"
                android:textSize="19sp" />

            <FrameLayout
                android:layout_width="58dp"
                android:layout_height="10dp"
                android:layout_gravity="center"
                android:layout_marginBottom="15dp"
                android:layout_marginTop="8dp" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/star_fill_img1"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill" />

                    <ImageView
                        android:id="@+id/star_fill_img2"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill" />

                    <ImageView
                        android:id="@+id/star_fill_img3"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill" />

                    <ImageView
                        android:id="@+id/star_fill_img4"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill" />

                    <ImageView
                        android:id="@+id/star_fill_img5"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/star_fill" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/star_stroke_img1"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/star_stroke_img2"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/star_stroke_img3"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/star_stroke_img4"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/star_stroke_img5"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/star_stroke" />
                </LinearLayout>
            </FrameLayout>

            <TextView
                android:id="@+id/tv_comment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="4dp"
                android:textColor="#474747"
                android:textSize="11sp" />

            <LinearLayout
                android:id="@+id/seasons"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="11dp"
                android:gravity="center"
                android:orientation="horizontal" >
            </LinearLayout>

            <TextView
                android:id="@+id/tv_comment2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="18dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="18dp" />

            <RelativeLayout
                android:id="@+id/rl_map"
                android:layout_width="fill_parent"
                android:layout_height="195dp"
                android:background="@android:color/black"
                android:paddingTop="1dip" >
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/tv_admintab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="15dp"
                    android:text="담당자"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="15dp"
                android:background="#868585" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/tv_costtab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="15dp"
                    android:text="가격"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_cost"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="15dp"
                    android:gravity="right"
                    android:text="가격"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="15dp"
                android:background="#868585" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/tv_leadtimetab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="15dp"
                    android:text="체험 시간"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_leadtime"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="15dp"
                    android:gravity="right"
                    android:text="체험 시간"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="15dp"
                android:background="#868585" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/tv_suppliestab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="15dp"
                    android:text="준비물"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_supplied"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="15dp"
                    android:text="준비물"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="15dp"
                android:background="#868585" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" >

                <TextView
                    android:id="@+id/tv_websitetab"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:layout_marginTop="15dp"
                    android:text="홈페이지"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/tv_website"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:layout_marginTop="15dp"
                    android:gravity="right"
                    android:text="홈페이지"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="16dp"
                android:layout_marginTop="15dp"
                android:background="#868585" >
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/iv_creategroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:src="@drawable/btn_opengroup" />

</LinearLayout>