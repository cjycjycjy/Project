<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical" >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:orientation="vertical" >

            <com.android.volley.toolbox.NetworkImageView
                android:id="@+id/fragment_admin_programinfo_challenge_image"
                android:layout_width="match_parent"
                android:layout_height="188dp"
                android:scaleType="centerCrop" />

            <LinearLayout
                android:gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_challenge_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#795548"
                    android:textSize="19sp" />

                <ImageView
                    android:layout_marginLeft="10dp"
                    android:id="@+id/fragment_admin_programinfo_modify"
                    android:layout_width="56dp"
                    android:layout_height="23dp"
                    android:src="@drawable/button_modify" />
            </LinearLayout>

            <FrameLayout
                android:layout_width="58dp"
                android:layout_height="10dp"
                android:layout_gravity="center"
                android:layout_marginTop="8dp" >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/star_stroke" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/fragment_admin_programinfo_star_fill_img1"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/fragment_admin_programinfo_star_fill_img2"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/fragment_admin_programinfo_star_fill_img3"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/fragment_admin_programinfo_star_fill_img4"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:layout_marginRight="1dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/fragment_admin_programinfo_star_fill_img5"
                        android:layout_width="10dp"
                        android:layout_height="10dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" />
                </LinearLayout>
            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="15dp"
                android:text="체험하기 좋은계절"
                android:textColor="#474747"
                android:textSize="11sp" />

            <LinearLayout
                android:id="@+id/seasons"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/fragment_admin_programinfo_img_spring"
                    android:layout_width="22dp"
                    android:layout_height="21dp"
                    android:src="@drawable/icon_spring"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/fragment_admin_programinfo_img_summer"
                    android:layout_width="22dp"
                    android:layout_height="21dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/icon_summer"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/fragment_admin_programinfo_img_autumn"
                    android:layout_width="22dp"
                    android:layout_height="21dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/icon_autumn"
                    android:visibility="gone" />

                <ImageView
                    android:id="@+id/fragment_admin_programinfo_img_winter"
                    android:layout_width="22dp"
                    android:layout_height="21dp"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/icon_winter"
                    android:visibility="gone" />
            </LinearLayout>

            <TextView
                android:id="@+id/fragment_admin_programinfo_tv_comment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="16dp"
                android:layout_marginRight="18dp"
                android:layout_marginTop="11dp" />

            <RelativeLayout
                android:id="@+id/fragment_admin_programinfo_rl_map"
                android:layout_width="fill_parent"
                android:layout_height="195dp"
                android:layout_marginTop="18dp"
                android:background="@android:color/black"
                android:paddingTop="1dip" >
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="담당자"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_masterid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="담당자아이디"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="가격"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_cost"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="가격"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="체험 시간"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_leadtime"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="체험 시간"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="준비물"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_supplied"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="준비물"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="홈페이지"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_website"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="홈페이지"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="연락처"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_phonenum"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="연락처"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="16dp"
                    android:text="교과과정"
                    android:textColor="#838486"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/fragment_admin_programinfo_tv_curriculum"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="16dp"
                    android:gravity="right"
                    android:text="교과과정"
                    android:textColor="#838486"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="22dp"
                android:background="#e1e1e1" >
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>