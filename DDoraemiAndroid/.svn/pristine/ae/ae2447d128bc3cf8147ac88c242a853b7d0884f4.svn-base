<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="138dp"
        android:background="@drawable/box_write"
        android:orientation="horizontal" >

        <FrameLayout
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" >

            <com.android.volley.toolbox.NetworkImageView
                android:id="@+id/item_mypage_pastlist_profileIMG"
                android:layout_width="84dp"
                android:layout_height="84dp"
                android:scaleType="centerCrop"
                android:src="@drawable/icon_user_profile" />

            <ImageView
                android:layout_width="84dp"
                android:layout_height="84dp"
                android:src="@drawable/circle_yellow" />
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="115dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/item_mypage_pastlist_programname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="z"
                android:textColor="#000000"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/item_mypage_pastlist_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:text="z"
                android:textColor="#474747"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/item_mypage_pastlist_Addr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:text="z"
                android:textColor="#474747"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="내 평점"
                    android:textColor="#000000"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/item_mypage_pastlist_scoreTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="0.0점"
                    android:textColor="#ffcc00"
                    android:textSize="14sp" />

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_pastlist_star1"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_pastlist_star2"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_pastlist_star3"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_pastlist_star4"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_pastlist_star5"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <ImageView
                    android:id="@+id/item_mypage_pastlist_questionimg"
                    android:layout_width="7dp"
                    android:layout_height="11dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/icon_questionmark" />
            </LinearLayout>
        </LinearLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="11dp"
            android:layout_marginTop="11dp" >

            <ImageView
                android:id="@+id/item_mypage_pastlist_delete"
                android:layout_width="58dp"
                android:layout_height="24dp"
                android:scaleType="fitXY"
                android:src="@drawable/btn_my_delete"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/item_mypage_pastlist_write"
                android:layout_width="58dp"
                android:layout_height="24dp"
                android:scaleType="fitXY"
                android:src="@drawable/button_write" />
        </FrameLayout>
    </RelativeLayout>

</LinearLayout>