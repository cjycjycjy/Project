package ddoraemi.detailedgroupinfo.model;

import java.io.Serializable;

public class QnAReply implements Serializable{
	int q_r_id;
	int q_id;
	String q_r_content;
	int q_r_year;
	int q_r_month;
	int q_r_day;
	String u_id;
	public QnAReply(int q_r_id, int q_id, String q_r_content, int q_r_year,
			int q_r_month, int q_r_day, String u_id) {
		super();
		this.q_r_id = q_r_id;
		this.q_id = q_id;
		this.q_r_content = q_r_content;
		this.q_r_year = q_r_year;
		this.q_r_month = q_r_month;
		this.q_r_day = q_r_day;
		this.u_id = u_id;
	}
	public int getQ_r_id() {
		return q_r_id;
	}
	public void setQ_r_id(int q_r_id) {
		this.q_r_id = q_r_id;
	}
	public int getQ_id() {
		return q_id;
	}
	public void setQ_id(int q_id) {
		this.q_id = q_id;
	}
	public String getQ_r_content() {
		return q_r_content;
	}
	public void setQ_r_content(String q_r_content) {
		this.q_r_content = q_r_content;
	}
	public int getQ_r_year() {
		return q_r_year;
	}
	public void setQ_r_year(int q_r_year) {
		this.q_r_year = q_r_year;
	}
	public int getQ_r_month() {
		return q_r_month;
	}
	public void setQ_r_month(int q_r_month) {
		this.q_r_month = q_r_month;
	}
	public int getQ_r_day() {
		return q_r_day;
	}
	public void setQ_r_day(int q_r_day) {
		this.q_r_day = q_r_day;
	}
	public String getU_id() {
		return u_id;
	}
	public void setU_id(String u_id) {
		this.u_id = u_id;
	}
	
	
}
