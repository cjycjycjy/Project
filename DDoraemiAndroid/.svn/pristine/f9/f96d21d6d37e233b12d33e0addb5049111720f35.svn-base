<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="138dp"
        android:background="@drawable/box_write"
        android:orientation="horizontal" >

        <FrameLayout
            android:layout_width="84dp"
            android:layout_height="84dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp" >

            <com.android.volley.toolbox.NetworkImageView
                android:id="@+id/item_mypage_wishlist_profileIMG"
                android:layout_width="84dp"
                android:layout_height="84dp"
                android:scaleType="centerCrop"
                android:src="@drawable/icon_user_profile" />

            <ImageView
                android:layout_width="84dp"
                android:layout_height="84dp"
                android:src="@drawable/circle_yellow" />
        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="115dp"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/item_mypage_wishlist_programname"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="z"
                android:textColor="#000000"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/item_mypage_wishlist_Sort"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:text="z"
                android:textColor="#474747"
                android:textSize="14sp" />

            <TextView
                android:id="@+id/item_mypage_wishlist_Addr"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="13dp"
                android:text="z"
                android:textColor="#474747"
                android:textSize="14sp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="총 평점"
                    android:textColor="#000000"
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/item_mypage_wishlist_scoreTv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp"
                    android:text="0.0점"
                    android:textColor="#ffcc00"
                    android:textSize="14sp" />

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="8dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_wishlist_star1"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_wishlist_star2"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_wishlist_star3"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_wishlist_star4"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="1dp" >

                    <ImageView
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_stroke" />

                    <ImageView
                        android:id="@+id/item_mypage_wishlist_star5"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:src="@drawable/star_fill"
                        android:visibility="gone" >
                    </ImageView>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/item_mypage_wishlist_delbookmarkImg"
            android:layout_width="18dp"
            android:layout_height="37dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="11dp"
            android:src="@drawable/icon_filledbookmark" />
    </RelativeLayout>

</LinearLayout>